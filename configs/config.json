{
  "status_http": {
    "addr": "{{ .status_http_listen_address }}"
  },
  "http": {
    "addr": "{{ .http_listen_address }}"
  },
  "log": {
    "level": "{{ .log_level }}"
  },
  "qr": {
    "url_pattern": "{{ .qr_url_pattern }}"
  },
  "telegram": {
    "listen_addr": "{{ .telegram_bot_listen_addr }}",
    "webhook_url": "{{ .telegram_bot_webhook_url }}",
    "token": "{{ .telegram_bot_token }}"
  },
  "storage": {
    "type": "{{ .storage_type }}",
    "mongo": {
      "uri": "{{ .mongo_uri }}"
    }
  },
  "redis": {
    "mode": "{{ .redis_mode }}",
    "addr": "{{ .redis_addr }}"
  }
}